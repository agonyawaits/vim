syntax on
set nocompatible
set number relativenumber
set noerrorbells
set tabstop=4 softtabstop=4
set expandtab
set smarttab 
set shiftwidth=4
set autoindent 
set cindent
set showmatch 
set ruler
set ignorecase
set wrap
set incsearch
set exrc
set wildmenu
set colorcolumn=90
set termguicolors

colorscheme alduin
let g:netrw_banner=0
let g:netrw_winsize=20
