syntax on
set nocompatible
set number relativenumber
set noerrorbells
set tabstop=4 expandtab shiftwidth=4 smarttab
set cindent
set showmatch 
set ruler
set ignorecase
set wrap
set incsearch
set exrc
set wildmenu
set colorcolumn=90
set termguicolors
colorscheme alduin
autocmd VimEnter * SetColors
let g:netrw_banner = 0
